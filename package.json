{
  "name": "rn-hack",
  "version": "1.0.0",
  "private": true,
  "description": "rn-hack",
  "author": "nakai-ryoichi",
  "keywords": [
    "gatsby"
  ],
  "compilerOptions": {
    "target": "es2021"
  },
  "babelMacros": {
    "twin": {
      "config": "./tailwind.config.js",
      "preset": "emotion",
      "hasSuggestions": true,
      "debug": false
    }
  },
  "scripts": {
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:eslint": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "format": "yarn format:eslint && yarn format:prettier"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/josefin-sans": "^4.5.9",
    "@fontsource/noto-sans-jp": "^4.5.10",
    "gatsby": "^4.13.1",
    "gatsby-plugin-emotion": "^7.14.0",
    "gatsby-plugin-google-analytics": "^4.13.0",
    "gatsby-plugin-image": "^2.13.0",
    "gatsby-plugin-manifest": "^4.16.0",
    "gatsby-plugin-offline": "^5.16.0",
    "gatsby-plugin-react-helmet": "^5.13.0",
    "gatsby-plugin-sass": "^5.13.0",
    "gatsby-plugin-sharp": "^4.13.0",
    "gatsby-plugin-sitemap": "^5.13.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^4.13.0",
    "gatsby-source-contentful": "^7.11.0",
    "gatsby-source-filesystem": "^4.13.0",
    "gatsby-transformer-remark": "^5.13.0",
    "gatsby-transformer-sharp": "^4.13.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "sass": "^1.51.0",
    "sharp": "^0.30.4",
    "twin.macro": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-webpack-plugin": "^3.1.1",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  },
  "resolutions": {
    "sharp": "^0.29.1",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.14"
  }
}
